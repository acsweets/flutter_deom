{
  "className": "Scaffold",
  "na": {
    "appBar": {
      "className": "AppBar",
      "na": {
        "title": {
          "className": "Text",
          "pa": [
            "GridView模版"
          ]
        }
      }
    },
    "floatingActionButton": {
      "className": "FloatingActionButton",
      "na": {
        "onPressed": "@(requestData)",
        "child": {
          "className": "Icon",
          "pa": [
            "#(Icons.add)"
          ]
        }
      }
    },
    "body": {
      "className": "Sugar.ifEqualBool",
      "pa": [
        "^(loading)"
      ],
      "na": {
        "falseValue": {
          "className": "Center",
          "na": {
            "child": {
              "className": "Text",
              "pa": [
                "加载中..."
              ]
            }
          }
        },
        "trueValue": {
          "className": "GridView.count",
          "na": {
            "crossAxisCount": 2,
            "mainAxisSpacing": 10,
            "crossAxisSpacing": 10,
            "children": {
              "className": "Sugar.map",
              "pa": [
                "^(_listData)"
              ],
              "na": {
                "builder": {
                  "className": "AspectRatio",
                  "na": {
                    "aspectRatio": 1.5,
                    "child": {
                      "className": "ClipRRect",
                      "na": {
                        "borderRadius": {
                          "className": "BorderRadius.all",
                          "pa": [
                            {
                              "className": "Radius.circular",
                              "pa": [
                                4.0
                              ]
                            }
                          ]
                        },
                        "child": {
                          "className": "Image.network",
                          "pa": [
                            "$(item.picUrl)"
                          ],
                          "na": {
                            "fit": "#(BoxFit.cover)"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "methodMap": {}
}